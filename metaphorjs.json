{
    "version": "0.1.7",
    "push": [
        "github"
    ],
    "build": {
        "standalone": {
            "wrap": true,
            "global": true,
            "target": "dist/metaphorjs.input.js",
            "files": [
                "../metaphorjs/src/MetaphorJs.js",
                [
                    "src/metaphorjs.input.js",
                    {
                        "as": "Input"
                    }
                ]
            ],
            "expose": [
                "Input"
            ]
        },
        "npm": {
            "target": "dist/metaphorjs.input.npm.js",
            "compile": false,
            "files": [
                [
                    "src/metaphorjs.input.js",
                    {
                        "keepExports": true
                    }
                ]
            ]
        },
        "amd": {
            "target": "dist/metaphorjs.input.amd.js",
            "files": [
                [
                    "src/metaphorjs.input.js",
                    {
                        "returnExports": true
                    }
                ]
            ],
            "define": {
                "name": "metaphorjs-input"
            }
        }
    }
}